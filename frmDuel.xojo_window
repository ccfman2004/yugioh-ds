#tag Events ABDuelArea
	#tag Event
		Sub MouseMove(X as integer, Y as integer)
		  dim elem as ABElement
		  elem = Me.ElementHit(X,Y)
		  Me.MouseCursor = curNormal
		  
		  if elem = nil then
		    LastElem = nil
		    blnSet = False
		    tmrCardDetails.Mode = 0
		    //me.MouseCursor = curNormal
		    
		  elseif lastelem = nil then
		    LastElem = elem
		    If elem.IsPopulated = True then
		      tmrCardDetails.Mode = 2
		      //me.MouseCursor = curNormal
		    Else
		      //me.MouseCursor = curNormal
		    End If
		  elseif elem <> lastelem then
		    LastElem = elem
		    blnSet = False
		    If elem.IsPopulated = True then
		      //tmrCardDetails.enabled = false
		      tmrCardDetails.Mode = 2
		      //me.MouseCursor = curNormal
		    Else
		      //me.MouseCursor = curNormal
		    End If
		  End if
		  
		  
		  If strCurrentPhase = "MP1" or strCurrentPhase = "MP2" Then
		    If elem <> Nil and elem.ID >= 100 Then
		      Select Case intCurrentPlayer
		      Case 0
		        If elem.Card.Level < 5 and elem.Card.Level > 0 and blnEnableNormalSummon and elem.ID < 200 and Not blnSet And intPlayer0MonsterZoneOccupation < 5 then 
		          me.MouseCursor = curSummon
		        ElseIf elem.Card.Level < 5 and elem.Card.Level > 0 and blnEnableNormalSummon and elem.ID < 200 And intPlayer0MonsterZoneOccupation < 5Then
		          me.MouseCursor = curSet
		        ElseIf elem.Card.Icon = "Field" then
		          me.MouseCursor = curActivate
		        End If
		        
		        
		        
		      Case 1
		        If elem.Card.Level < 5 and elem.Card.Level > 0 and blnEnableNormalSummon and elem.ID >= 200 then me.MouseCursor = curSummon
		        
		        
		        
		      End Select
		      
		    Else
		    End If
		  End If
		End Sub
	#tag EndEvent
	#tag Event
		Sub MouseExit()
		  LastElem = Nil
		  Me.MouseCursor = curNormal
		End Sub
	#tag EndEvent
	#tag Event
		Function MouseDown(x as integer, y as integer) As boolean
		  dim elem as ABElement
		  elem = Me.ElementHit(X,Y)
		  Me.MouseCursor = curNormalClick
		  
		  
		  If IsContextualClick Then 
		    blnContexualClick = True
		  Else 
		    blnContexualClick = False
		  End If
		  
		  
		  If strCurrentPhase = "MP1" or strCurrentPhase = "MP2" Then
		    If elem <> Nil and elem.ID >= 100 Then
		      Select Case intCurrentPlayer
		      Case 0
		        If elem.Card.Level < 5 and elem.Card.Level > 0 and blnEnableNormalSummon and elem.ID < 200 and Not blnSet And intPlayer0MonsterZoneOccupation < 5 then 
		          me.MouseCursor = curSummonClick
		          
		          Return True
		          
		        ElseIf elem.Card.Level < 5 and elem.Card.Level > 0 and blnEnableNormalSummon and elem.ID < 200 And intPlayer0MonsterZoneOccupation < 5 Then
		          me.MouseCursor = curSetClick
		          
		        ElseIf elem.Card.Icon = "Field" Then
		          me.MouseCursor = curActivateClick
		          
		        Else
		          Me.MouseCursor = curNormalClick
		        End If
		        
		        
		        
		      Case 1
		        If elem.Card.Level < 5 and elem.Card.Level > 0 and blnEnableNormalSummon and elem.ID >= 200 and Not blnSet then 
		          me.MouseCursor = curSummonClick
		        ElseIf elem.Card.Level < 5 and elem.Card.Level > 0 and blnEnableNormalSummon and elem.ID >= 200 Then
		          me.MouseCursor = curSetClick
		        Else
		          Me.MouseCursor = curNormalClick
		        End If
		        
		        
		        
		      End Select
		    Else
		      Me.MouseCursor = curNormalClick
		    End If
		  Else
		  End If
		  
		  
		  Return True
		End Function
	#tag EndEvent
	#tag Event
		Sub MouseUp(X as integer, Y as integer)
		  dim elem as ABElement
		  elem = Me.ElementHit(X,Y)
		  Me.MouseCursor = curNormal
		  
		  
		  If strCurrentPhase = "MP1" or strCurrentPhase = "MP2" Then
		    If elem <> Nil and elem.ID >= 100 Then
		      If blnContexualClick Then
		        Select Case intCurrentPlayer
		        Case 0
		          
		          If elem.Card.Level < 5 and elem.Card.Level > 0 and blnEnableNormalSummon and elem.ID < 200 and not blnSet And intPlayer0MonsterZoneOccupation < 5 then 
		            me.MouseCursor = curSet
		            blnSet = True
		          ElseIf elem.Card.Level < 5 and elem.Card.Level > 0 and blnEnableNormalSummon and elem.ID < 200 and blnSet And intPlayer0MonsterZoneOccupation < 5 Then
		            me.MouseCursor = curSummon
		            blnSet = False
		            
		          Else
		            Me.MouseCursor = curNormal
		          End If
		          
		          
		          
		        Case 1
		          If elem.Card.Level < 5 and elem.Card.Level > 0 and blnEnableNormalSummon and elem.ID >= 200 And Not blnSet then 
		            me.MouseCursor = curSet
		            blnSet = True
		          ElseIf elem.Card.Level < 5 and elem.Card.Level > 0 and blnEnableNormalSummon and elem.ID >= 200 And Not blnSet Then
		            me.MouseCursor = curSummon
		            blnSet = False
		          Else
		            Me.MouseCursor = curNormal
		          End If
		          
		          
		        End Select
		      Else
		        If elem <> Nil and elem.ID >= 100 Then
		          Select Case intCurrentPlayer
		          Case 0
		            If elem.Card.Level < 5 and elem.Card.Level > 0 and blnEnableNormalSummon and elem.ID < 200 and Not blnSet And intPlayer0MonsterZoneOccupation < 5 then 
		              me.MouseCursor = curSummon
		              
		              SummonMonster(0, 0, elem.HandID)
		              
		            ElseIf elem.Card.Level < 5 and elem.Card.Level > 0 and blnEnableNormalSummon and elem.ID < 200 And intPlayer0MonsterZoneOccupation < 5 Then
		              me.MouseCursor = curSet
		              
		            ElseIf elem.Card.Icon = "Field" Then
		              ActivateField(0, 0, elem.HandID)
		              
		            End If
		            
		            
		            
		          Case 1
		            If elem.Card.Level < 5 and elem.Card.Level > 0 and blnEnableNormalSummon and elem.ID >= 200 then me.MouseCursor = curSummon
		            
		            
		            
		          End Select
		        Else
		          Me.MouseCursor = curNormal
		        End If
		      End If
		    Else
		      Me.MouseCursor = curNormal
		    End If
		  Else
		  End If
		End Sub

		Sub Action()
		  If intDrawNumber <> 0 then
		    
		    DrawCard(intPlayer, 1)
		    intDrawNumber = intDrawNumber - 1
		    
		  Else
		    Me.Mode = 0
		    
		  End If
		End Sub

		Private Sub Populate(crdCard As Card)
		  tmpCardPic = crdCard.Pic(0)
		  imgCard.Invalidate(False)
		  
		  lblCardName.Text = crdCard.Name
		  lblArcheType.Text = crdCard.Archetype
		  lblType.Text = crdCard.Type
		  lblAttribute.Text = crdCard.Attribute
		  
		  //Check if card is a Monster
		  If crdCard.Attribute <> "Spell" and crdCard.Attribute <> "Trap" Then 
		    lblATK.Text = "ATK/ " + crdCard.ATK
		    lblDEF.Text = "DEF/ " + crdCard.DEF
		  Else
		    lblATK.Text = ""
		    lblDEF.Text = ""
		  End If
		  
		  //Check if Monster is Normal
		  If crdCard.Frame = "Normal" Then
		    lblDesc.Italic = True
		  Else
		    lblDesc.Italic = False
		  End
		  
		  lblDesc.Text = EndOfLine + crdCard.Desc
		End Sub

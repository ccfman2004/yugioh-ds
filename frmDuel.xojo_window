#tag Events tmrDrawInitialHand
	#tag Event
		Sub Action()
		  If intAllPlayersDealt <> 2 Then
		    
		    If intDrawNumber <> 0 then
		      
		      DrawCard(intCurrentPlayer, 1)
		      intDrawNumber = intDrawNumber - 1
		    End If
		    
		    If intCurrentPlayer = 0 And intDrawNumber = 0 then
		      intCurrentPlayer = 1
		      intDrawNumber = 5
		      intAllPlayersDealt = intAllPlayersDealt + 1
		    ElseIf intDrawNumber = 0 Then
		      intCurrentPlayer = 0
		      intDrawNumber = 3
		      intAllPlayersDealt = intAllPlayersDealt + 1
		    End If
		    
		  Else
		    tmrDrawInitialHand.Mode = 0
		    ABDuelArea.Enabled = True
		    tmrDrawPhase.Mode = 1
		  End If
		End Sub
	#tag EndEvent

	#tag Method, Flags = &h21
		Private Sub RepositionHands(iPlayer As Integer)
		  Dim a As Integer
		  Dim b As Integer
		  Dim tmpElem As ABElement
		  Dim c As Integer
		  
		  If crdPlayer0Hand.Ubound <= 7 then
		    c = 75
		  ElseIf crdPlayer0Hand.Ubound > 7 And crdPlayer0Hand.Ubound <=11 Then
		    c = 50
		  ElseIf crdPlayer0Hand.Ubound >11 Then
		    c = 30
		  End If
		  
		  Select Case iPlayer
		  Case 0
		    
		    For a = 0 to ABDuelArea.MyElements.Ubound
		      
		      If ABDuelArea.MyElements(a).ID >= 100 And ABDuelArea.MyElements(a).ID < 200 Then 
		        
		        ABDuelArea.MyElements.Remove(a)
		        a = a - 1
		        
		      End If
		      
		    Next a
		    
		    For b = 0 to crdPlayer0Hand.Ubound
		      
		      crdPlayer0Hand(b).ElementID = b + 100
		      tmpElem = New ABElement(ABDuelArea)
		      tmpElem.Init(crdPlayer0Hand(b).ElementID, 137 + (c*b) , 729, crdPlayer0Hand(b).Pic(0).ScaleMBS(100,146), True, True)
		      tmpElem.Card = crdPlayer0Hand(b)
		      tmpElem.HandID = b
		      tmpElem.IsPopulated = True
		      ABDuelArea.AddElement(tmpElem)
		      
		    Next b
		    
		    
		  Case 1
		    
		    
		  End Select
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h21
		Private Sub ShuffleDeck(intPlayer As Integer)
		  If intPlayer = 0 then
		    crdPlayer0Deck.Shuffle
		  Else
		    crdPlayer1Deck.Shuffle
		  End If
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h21
		Private Sub SummonMonster(intFieldSide As Integer, intWhooseHand As Integer, intHandPosition As Integer)
		  Dim i As Integer
		  Dim a As Integer
		  
		  For i = 0 to 4
		    
		    If crdPlayer0MonsterZone(i) = Nil Then
		      crdPlayer0MonsterZone(i) = crdPlayer0Hand(intHandPosition)
		      crdPlayer0Hand.Remove(intHandPosition)
		      
		      
		      For a = 0 to ABDuelArea.MyElements.Ubound
		        If ABDuelArea.MyElements(a).Card = crdPlayer0MonsterZone(i) then
		          ABDuelArea.MyElements.Remove(a)
		        End If
		      Next
		      
		      ABDuelArea.MyElements(i+20).Card = crdPlayer0MonsterZone(i)
		      ABDuelArea.MyElements(i+20).pBuffer.Graphics.DrawPicture(crdPlayer0MonsterZone(i).SmPic.ScaleMBS(50, 72),25 , 6)
		      ABDuelArea.MyElements(i+20).IsPopulated = True
		      
		      RepositionHands(intWhooseHand)
		      
		      ABDuelArea.DrawMe(0, 0, 766, 768)
		      
		      blnEnableNormalSummon = False
		      
		      intPlayer0MonsterZoneOccupation = intPlayer0MonsterZoneOccupation + 1
		      
		      
		      
		      Return
		      
		      
		      
		    End If
		    
		  Next i
		End Sub
	#tag EndMethod

